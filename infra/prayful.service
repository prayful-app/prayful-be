[Unit]
Description=Prayful API
After=network.target

[Service]
Type=simple
User=ubuntu
WorkingDirectory=/home/ubuntu/repos/prayful-be
ExecStart=/home/ubuntu/.sdkman/candidates/java/current/bin/java /home/ubuntu/repos/prayful-be/webapp/target/webapp-1.0-SNAPSHOT.jar
Restart=always

[Install]
WantedBy=multi-user.target